<p><%= notice %></p>

<h3>Date: <%= @daily_log.date %> </h3>

<h2>Daily Logs:</h2>
<% @daily_log.log_entries.each do |log_entry| %>
  <%= link_to log_entry.food_name, daily_log_log_entry_path(@daily_log, log_entry) %>
<% end %>

<%= link_to "Create new Log Entry", new_daily_log_log_entry_path(@daily_log) %>
<div>

  <%= link_to "Edit this daily log", edit_daily_log_path(@daily_log) %> |
  <%= link_to "Back to daily logs", daily_logs_path %>

  <%= button_to "Destroy this daily log", @daily_log, method: :delete %>
</div>
